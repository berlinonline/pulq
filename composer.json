{
    "name": "berlinonline/pulq",
    "type": "project",
    "description": "Agavi based framework for building frontend applications.",
    "keywords": ["agavi", "mvc" , "frontend"],
    "homepage": "https://github.com/berlinonline/pulq",
    "license": "MIT",
    "authors": [
        {
            "name": "Leon Weidauer"
        },
        {
            "name": "Tom Anheyer"
        },
        {
            "name": "Thorsten Schmitt-Rink"
        }
    ],

    "autoload": {
        "psr-0": {"Pulq" : "app/lib" }
    },

    "minimum-stability": "stable",

    "require": {
        "php": ">=5.3.3",
        "honeybee/agavi": "1.1.*",
        "ruflin/elastica": "0.90.*",
        "PhpDebugToolbar/php-debug-toolbar": "1.6.*",
        "twig/twig": "1.21.*",
        "graste/environaut": "~0.3",
        "psr/log": "1.0.0",
        "monolog/monolog": "1.5",
        "symfony/console": "~2.7",
        "csswizardry/inuit.css": "5.0.1",
        "michelf/php-markdown": "~1.0"
    },

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "PhpDebugToolbar/php-debug-toolbar",
                "version": "1.6.0",
                "source": {
                    "url": "https://github.com/DracoBlue/PhpDebugToolbar.git",
                    "type": "git",
                    "reference": "8f829a838dc6dee69dbbc4a4af28295bf07abd0a"
                },
                "autoload": {
                    "classmap": ["lib/agavi/", "PhpDebugToolbar.class.php"]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "csswizardry/inuit.css",
                "version": "5.0.1",
                "source": {
                    "url": "https://github.com/csswizardry/inuit.css.git",
                    "type": "git",
                    "reference": "aa49931763f8ef8b1bf7780108a84fc75a099a66"
                }
            }
        }
    ]
}
